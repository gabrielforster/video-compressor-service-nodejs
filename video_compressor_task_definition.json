{
    "containerDefinitions": [
        {
            "name": "video_compressor",
            "image": "018970712967.dkr.ecr.sa-east-1.amazonaws.com/video_compressor_nodejs:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "http_server",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                },
                {
                    "name": "websocket_server",
                    "containerPort": 81,
                    "hostPort": 81,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/video_compressor_task_definition",
                    "awslogs-region": "sa-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "family": "video_compressor_task_definition",
    "taskRoleArn": "arn:aws:iam::018970712967:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::018970712967:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "8192",
    "memory": "16384",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}
